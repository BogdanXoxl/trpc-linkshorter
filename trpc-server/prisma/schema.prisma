generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id             String   @id @default(cuid())
  href           String   @unique
  shortLink      String   @unique
  expirationDate DateTime @default(dbgenerated("(NOW() + '15 minutes'::interval)"))
}
