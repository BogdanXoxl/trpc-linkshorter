{
  "version": 2,
  "outputDirectory": "dist",
  "buildCommand": "echo build",
  "rewrites": [
    { "source": "/trpc/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" },
    { "source": "/api(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/index.html" }
  ]
}
