{
  "version": 2,
  "outputDirectory": "dist",
  "buildCommand": "echo build",
  "rewrites": [
    { "source": "/(.*)", "status": 404, "destination": "/index.html" },
    { "source": "/trpc/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" },
    { "source": "/api(.*)", "destination": "/index.html" }
  ]
}
